const pi = 3.1415926

class Circle {
    init(x, y, radius) {
        self.x = x
        self.y = y
        self.radius = radius
    }

    area() { return pi * self.radius ^ 2 }
    diameter() { return 2 * self.radius }
    circumference() { return 2 * pi * self.radius }
}

const circle = Circle(50, 50, 20)

print("Area: " + circle.area())
print("Diameter: " + circle.diameter())
print("Circumference: " + circle.circumference())